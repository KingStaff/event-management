{
    "builds": [{
        "src": "django-event-management-master/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "django-event-management-master/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "django-event-management-master/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "django-event-management-master/wsgi.py"
        }
    ]
}
